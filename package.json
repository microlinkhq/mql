{
  "name": "@microlink/mql",
  "description": "Microlink Query Language",
  "homepage": "https://nicedoc.io/microlinkhq/mql",
  "version": "0.4.2",
  "browser": "src/browser.js",
  "main": "src/node.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/mql.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/mql/issues"
  },
  "keywords": [
    "api",
    "language",
    "microlink",
    "mql",
    "query"
  ],
  "dependencies": {
    "flat": "~4.1.0",
    "got": "~9.6.0",
    "is-url-http": "~1.2.0",
    "ky": "~0.14.0",
    "ky-universal": "~0.3.0",
    "qss": "~2.0.3",
    "whoops": "~4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "beauty-error": "latest",
    "chalk": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "eachdir": "latest",
    "esm": "latest",
    "exists-file": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "meow": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "prettier-standard": "latest",
    "rollup": "latest",
    "rollup-plugin-alias": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-filesize": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-plugin-replace": "latest",
    "rollup-plugin-shim": "latest",
    "rollup-plugin-terser": "latest",
    "rollup-plugin-visualizer": "latest",
    "standard": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean": "rm -rf node_modules",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard",
    "postrelease": "npm run release:build && npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "git-dirty && npm run update:check && npm test",
    "pretest": "npm run lint",
    "release": "git-authors-cli && finepack && git add package.json && standard-version -a",
    "release:build": "npm run build && git add dist/ && git commit -m \"build: generate bundle\"",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin master",
    "test": "nyc ava --verbose",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "ava": {
    "require": [
      "esm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js,!*.min.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "umd:main": "dist/mql.min.js",
  "unpkg": "dist/mql.min.js"
}
